apiVersion: flux.weave.works/v1beta1
kind: HelmRelease
metadata:
  name: kube-eagle
  namespace: default
  annotations:
    flux.weave.works/automated: "true"
    flux.weave.works/tag: "glob:*"
spec:
  releaseName: kube-eagle
  chart:
    git: git@github.com:google-cloud-tools/kube-eagle-helm-chart
    path: kube-eagle
    ref: master
  values:
    serviceMonitor:
      create: false
    image:
      tag: v1.1.1
